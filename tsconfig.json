{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "types": [
      "svelte"
    ],
    "alwaysStrict": true,
    "allowJs": true,
    "target": "esnext",
    "isolatedModules": true,
    "resolveJsonModule": true,
  }
}